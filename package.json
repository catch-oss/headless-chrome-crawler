{
  "name": "@popstas/headless-chrome-crawler",
  "version": "1.9.0",
  "description": "Distributed web crawler powered by Headless Chrome",
  "main": "index.js",
  "license": "MIT",
  "author": "Yuji Isobe",
  "scripts": {
    "commitmsg": "commitlint -e",
    "prepare": "husky",
    "lint": "eslint --quiet -f codeframe . || eslint .",
    "jest-exclude-redis-cache": "yarn jest $(find test -name '*.test.js' -not -name 'redis.test.js')",
    "jest": "jest --maxWorkers=1",
    "test": "yarn tsc && yarn lint && yarn jest-exclude-redis-cache",
    "chromium:dir": "ls -la tmp/chromium/chromium | grep -m 1 mac | awk '{print $9}'",
    "test:macos:chromium": "yarn dlx @puppeteer/browsers install chromium@latest --path ./tmp/chromium && CHROMIUM_DIR=$(yarn run chromium:dir) && export PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true && export PUPPETEER_EXECUTABLE_PATH=\"$(pwd)/tmp/chromium/chromium/$CHROMIUM_DIR/chrome-mac/Chromium.app/Contents/MacOS/Chromium\" && yarn test",
    "test:macos:chrome": "export PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true && export PUPPETEER_EXECUTABLE_PATH=\"/Applications/Google Chrome.app/Contents/MacOS/Google Chrome\" && yarn test",
    "test-all": "yarn tsc && yarn lint && yarn jest",
    "tsc": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/popstas/headless-chrome-crawler.git"
  },
  "bugs": {
    "url": "https://github.com/popstas/headless-chrome-crawler/issues"
  },
  "homepage": "https://github.com/popstas/headless-chrome-crawler#readme",
  "dependencies": {
    "axios": "^1.12.2",
    "debug": "4.4.3",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "puppeteer": "^24.23.0",
    "robots-parser": "3.0.1"
  },
  "peerDependencies": {
    "redis": "2.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "9.36.0",
    "@types/debug": "4.1.12",
    "@types/lodash": "4.17.20",
    "@types/node": "24.6.2",
    "@types/puppeteer": "7.0.4",
    "@types/request-promise": "4.1.51",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "9.1.7",
    "jest": "^30.2.0",
    "mime-types": "^2.1.35",
    "redis": "^5.8.3",
    "typescript": "5.9.3"
  },
  "keywords": [
    "headless",
    "chrome",
    "crawler",
    "crawling",
    "jquery",
    "promise",
    "puppeteer",
    "scraper",
    "scraping"
  ],
  "engines": {
    "node": ">=8.10.0"
  },
  "packageManager": "yarn@4.10.2+sha512.0b0e54ad5381f0a35184957bd3ea44e37f5a4fb1960b903b0fb9a3c7c2c009190455c41ef2a1977b1dbd3b72c5f152da696e9c52e2bc78a011b6adea8ee73ba3"
}
